{
    "name": "TranslateNowProto",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
      "activeTab",
      "tabs",
      "storage"
      ],
    "description": "Prototype of ASL Translation Extension",
    "action": {
        "default_icon": "SignWaveHandTransparent.png",
        "default_popup": "popup.html"
    },
    "options_page": "options.html",
    "background": {
      "service_worker": "background.js"
    }
}
